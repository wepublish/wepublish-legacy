{
  "name": "@wepublish/example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "webpack client=./src/client.js worker=./src/worker.js --mode production",
    "watch": "run-p watch:*",
    "watch:client": "webpack client=./src/client.js worker=./src/worker.js --mode development --watch",
    "watch:server": "nodemon server.js",
    "start": "node ./server.js",
    "sync-package-info": "exit 0"
  },
  "dependencies": {
    "@wepublish/server": "0.0.0",
    "dotenv-safe": "^6.1.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "@wepublish/client": "0.0.0",
    "@wepublish/worker": "0.0.0",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "nodemon": "^1.18.9"
  }
}
